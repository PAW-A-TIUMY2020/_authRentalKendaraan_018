

<from asp-controller="Upload" asp-action="UploadFile" method="post" enctype="multipart/form-data">
    <input type="file" name="file"/>
    <button type="submit">Upload File</button>
</from>

<from asp-controller="Upload" asp-action="UploadFiles" method="post" enctype="multipart/from-data">
    <input type="file" name="files" multiple/>
    <button type="submit">Upload Files</button>
</from>

<from asp-controller="Upload" asp-action="UploadFileViaModel" method="post" enctype="multipart/from-data">
    <input type="file" name="FileToUpload"/>
    <button type="submit">Upload Files (model)</button>
</from>
